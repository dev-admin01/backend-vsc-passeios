{
  "name": "backend-vsc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && npm run services:wait:database && npm run migrations:up && vercel dev",
    "services:up": "docker compose up -d",
    "services:down": "docker compose down",
    "services:stop": "docker compose stop",
    "test:watch": "npx jest --watch --runInBand",
    "build": "tsc",
    "postinstall": "prisma generate && prisma migrate deploy && prisma db seed",
    "services:wait:database": "node infra/scripts/wait-for-postgres.js",
    "migrations:up": "npx prisma migrate dev",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "npx eslint ."
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "bcrypt": "^5.1.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}